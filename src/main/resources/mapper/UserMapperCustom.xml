<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yksj.monitor.mapper.StuMapperCustom">
    <select id="getStuById" parameterType="String" resultType="com.yksj.monitor.entity.Stu">
        SELECT id, `name`, `age`, `email`
        from `user`
        WHERE `id` = #{sid};
    </select>
</mapper>